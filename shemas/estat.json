{
  "openapi": "3.1.0",
  "info": {
    "title": "e-Stat API",
    "description": "政府統計の総合窓口（e-Stat）のAPI機能 - メタ情報取得とデータ取得（JSON形式のみ）",
    "version": "v3.0.0"
  },
  "servers": [
    {
      "url": "https://api.e-stat.go.jp/rest/3.0/app/json",
      "description": "e-Stat API v3.0 JSON"
    }
  ],
  "paths": {
    "/getMetaInfo": {
      "get": {
        "summary": "メタ情報取得（JSON形式）",
        "description": "指定した統計表IDに対応するメタ情報（表章事項、分類事項、地域事項等）をJSON形式で取得します。",
        "operationId": "getMetaInfo",
        "parameters": [
          {
            "name": "appId",
            "in": "query",
            "description": "アプリケーションID（必須）",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "statsDataId",
            "in": "query",
            "description": "統計表ID（必須）",
            "required": true,
            "schema": {
              "type": "string",
              "pattern": "^[0-9]{10}$"
            }
          },
          {
            "name": "lang",
            "in": "query",
            "description": "言語（J:日本語、E:英語）",
            "required": false,
            "schema": {
              "type": "string",
              "enum": ["J", "E"],
              "default": "J"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "正常終了",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/MetaInfoResponse"
                }
              }
            }
          },
          "400": {
            "$ref": "#/components/responses/BadRequest"
          },
          "403": {
            "$ref": "#/components/responses/Unauthorized"
          },
          "500": {
            "$ref": "#/components/responses/InternalServerError"
          }
        }
      }
    },
